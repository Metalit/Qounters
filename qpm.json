{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Qounters++",
    "id": "qounters++",
    "version": "1.0.0",
    "url": "https://github.com/Metalit/QountersPlusPlus",
    "additionalData": {
      "overrideSoName": "libqountersplusplus.so"
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1 $0?"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?",
        "pwsh ./scripts/restart-game.ps1"
      ],
      "log": [
        "pwsh ./scripts/start-logging.ps1 $0:"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1 $0?",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1 $0?"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1 -analyze"
      ]
    },
    "qmodIncludeDirs": [],
    "qmodIncludeFiles": [],
    "qmodOutput": null
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.1.9",
      "additionalData": {
        "extraFiles": [
          "src/inline-hook"
        ]
      }
    },
    {
      "id": "bs-cordl",
      "versionRange": "^3700.0.0",
      "additionalData": {}
    },
    {
      "id": "paper",
      "versionRange": "^3.6.1",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.0",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "custom-types",
      "versionRange": "^0.17.10",
      "additionalData": {}
    },
    {
      "id": "rapidjson-macros",
      "versionRange": "^2.0.0",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^2.0.0",
      "additionalData": {}
    },
    {
      "id": "song-details",
      "versionRange": "^0.3.1",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.18",
      "additionalData": {}
    },
    {
      "id": "web-utils",
      "versionRange": "^0.6.6",
      "additionalData": {}
    },
    {
      "id": "songcore",
      "versionRange": "^1.1.15",
      "additionalData": {}
    }
  ]
}
